# Copy this file to .env.local and update with your ngrok URLs
# After running: ngrok start --all --config ../ngrok.yml

# Frontend will use localhost by default
# Only set these when exposing via ngrok for external access

# Main backend API (tenant-service)
VITE_TENANT_API_URL=https://your-tenant-api.ngrok.io

# LLM Service
VITE_LLM_API_URL=https://your-llm-api.ngrok.io

# Booking Service
VITE_BOOKING_API_URL=https://your-booking-api.ngrok.io

# Order Service
VITE_ORDER_API_URL=https://your-order-api.ngrok.io

# Optional: Other services (uncomment if needed)
